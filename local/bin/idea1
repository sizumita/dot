#!/bin/bash
#Generated by JetBrains Toolbox 2.3.1.31116 at 2024-05-09T12:06:02.565818

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/sizumita/Applications/IntelliJ IDEA Ultimate 2023.3.6.app/Contents/MacOS/idea" $wait --args "${intellij_args[@]}"